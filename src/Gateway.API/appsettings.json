{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Redis": "localhost:6379"
  },
  "JwtOption": {
    "Issuer": "http://localhost:5000",
    "Audience": "http://localhost:5000",
    "SecretKey": "oEZqUKRrKDKP7A9OtrB5GfPGJ92vLDlK",
    // Remove this on production environment
    "ExpireMin": 60
  },
  "ClientApp": "https://localhost:3000",
  "ReverseProxy": {
    "Routes": {
      "fuc": {
        "ClusterId": "fuc",
        "CorsPolicy": "customPolicy",
        "Match": {
          "Path": "/fuc/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "api/fuc/{**catch-all}"
          }
        ]
      },
      "identity": {
        "ClusterId": "identity",
        "CorsPolicy": "customPolicy",
        "Match": {
          "Path": "/identity/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "api/identity/{**catch-all}"
          }
        ]
      },
      "notifications": {
        "ClusterId": "notifications",
        "CorsPolicy": "customPolicy",
        "Match": {
          "Path": "/notifications/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "fuc": {
        "Destinations": {
          "fucApi": {
            "Address": "https://localhost:7000"
          }
        }
      },
      "identity": {
        "Destinations": {
          "identityApi": {
            "Address": "https://localhost:5001"
          }
        }
      },
      "notifications": {
        "Destinations": {
          "notifyApi": {
            "Address": "https://localhost:7004"
          }
        }
      }
    }
  }
}
